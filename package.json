{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "shopify-ai",
  "title": "Shopify AI",
  "description": "Query Shopify AI model directly from Raycast",
  "icon": "shopify-favicon.png",
  "author": "your-name",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Ask Shopify AI",
      "description": "Ask a question to Shopify AI",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.50.3",
    "@google/genai": "^0.12.0",
    "@raycast/api": "^1.96.0",
    "mergerino": "^0.4.0",
    "openai": "^4.97.0",
    "staterino": "^2.0.0"
  },
  "devDependencies": {
    "@types/mergerino": "^0.4.5",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false
  }
}
